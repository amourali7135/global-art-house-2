  </div>
    </div>
    <div class="carousel-item">
      <%= cl_image_tag(art.photos.second,
      width: 200, height: 150, crop: :fill, class: "d-block w-100") %>
      <div class="carousel-caption d-none d-md-block">
    <h5><%= art.title %></h5>
