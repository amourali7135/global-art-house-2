<h1> Hello <%= @artist.first_name %>! </h1>

     <%= cl_image_tag(@artist.photo,
    width: 150, height: 100, crop: :fill) %>
<p>Share new art <%= link_to "Add Art", new_artist_art_path(@artist.id), class: 'btn btn-primary' %> </p>

Your artist tags
 <div class="my-5">
      <% @artist.tags.each do |tag| %>
        <p class="mb-0">TAG: <%= tag.name %></p>
      <% end %>
    </div>
    <p><small>Tags: <%= raw @artist.tags.map(&:name).map { |t| link_to t, tag_path(t) }.join(', ') %></small></p>

<p>See all of your existing art  <%= link_to "Your art", artist_arts_path(@artist.id), class: 'btn btn-primary'  %>   </p>

<% @artist.arts.each do |art| %>
   <div>
    <%= render partial: 'shared/art_card', locals: {art: art} %>
  </div>
<% end %>
